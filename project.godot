[autoload]

Globals="*res://scenes/managers/globals.gd"
AudioManager="*res://scenes/managers/audio_manager.gd"
LevelManager="*res://scenes/managers/level_manager.gd"
GitStatus="*res://addons/git-status/utility.gd"
XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
XRToolsRumbleManager="*res://addons/godot-xr-tools/rumble/rumble_manager.gd"

[debug]

